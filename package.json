{
  "name": "testcafe-reporter-saucelabs",
  "version": "1.0.1",
  "description": "Sauce Labs TestCafe reporter plugin.",
  "repository": "https://github.com/saucelabs/testcafe-reporter",
  "author": {
    "name": "devx",
    "email": "devx@saucelabs.com",
    "url": "https://www.saucelabs.com"
  },
  "main": "dist/src/index.js",
  "files": [
    "dist/src"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc",
    "watch": "tsc -w",
    "lint": "eslint src/",
    "test": "jest",
    "release": "tsc && release-it --github.release",
    "release:ci": "tsc && npm run release -- --ci --no-git.requireCleanWorkingDir",
    "release:patch": "npm run release -- patch",
    "release:minor": "npm run release -- minor",
    "release:major": "npm run release -- major"
  },
  "keywords": [
    "saucelabs",
    "testcafe",
    "reporter",
    "plugin"
  ],
  "license": "MIT",
  "devDependencies": {
    "@tsconfig/node14": "^1.0.3",
    "@types/debug": "^4.1.7",
    "@types/jest": "^29.4.0",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "jest": "^29.5.0",
    "release-it": "^15.5.0",
    "testcafe": "^2.0.1",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "testcafe": ">=1.0.1"
  },
  "dependencies": {
    "@saucelabs/sauce-json-reporter": "^1.0.0",
    "@saucelabs/testcomposer": "^0.1.0",
    "axios": "^1.2.4",
    "debug": "^4.3.4",
    "ts-jest": "^29.0.5"
  }
}
